<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".TestActivityIntent">

    <Button
        android:id="@+id/button1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="L'équipe"
        tools:layout_editor_absoluteX="148dp"
        tools:layout_editor_absoluteY="154dp" />
    <Button
        android:id="@+id/button2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Lancer un appel"
        tools:layout_editor_absoluteX="148dp"
        tools:layout_editor_absoluteY="154dp" />
    <Button
        android:id="@+id/button3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Préparer l'appel"
        tools:layout_editor_absoluteX="148dp"
        tools:layout_editor_absoluteY="154dp" />
    <Button
        android:id="@+id/button4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Screenshot"
        tools:layout_editor_absoluteX="148dp"
        tools:layout_editor_absoluteY="154dp" />
    <Button
        android:id="@+id/button5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Affiche Liste Contacts"
        tools:layout_editor_absoluteX="148dp"
        tools:layout_editor_absoluteY="154dp" />
    <Button
        android:id="@+id/button6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Test ListView"
        tools:layout_editor_absoluteX="148dp"
        tools:layout_editor_absoluteY="154dp" />
    <Button
        android:id="@+id/button7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Test SimpleAdapter"
        tools:layout_editor_absoluteX="148dp"
        tools:layout_editor_absoluteY="154dp" />
    <Button
        android:id="@+id/button8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="LivreActivity"
        tools:layout_editor_absoluteX="148dp"
        tools:layout_editor_absoluteY="154dp" />

</LinearLayout>